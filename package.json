{
	"name": "sveltekit-template",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "pnpm dlx @biomejs/biome lint ./src",
		"lint:apply": "pnpm dlx @biomejs/biome lint --apply ./src",
		"format": "pnpm dlx @biomejs/biome format ./src --write",
		"format:lint": "pnpm dlx @biomejs/biome check ./src",
		"migrate": "drizzle-kit generate:pg --config=drizzle.config.ts",
		"migration:push": "drizzle-kit push:pg --config=drizzle.config.ts",
		"db:studio": "drizzle-kit studio"
	},
	"devDependencies": {
		"@biomejs/biome": "1.4.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/nodemailer": "^6.4.14",
		"@types/pg": "^8.10.9",
		"autoprefixer": "^10.4.14",
		"drizzle-kit": "^0.20.10",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prisma": "^5.7.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"sveltekit-flash-message": "^2.3.0",
		"sveltekit-superforms": "^1.12.0",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.0",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@internationalized/date": "^3.5.0",
		"@lucia-auth/adapter-postgresql": "^2.0.2",
		"bits-ui": "^0.11.7",
		"clsx": "^2.0.0",
		"cmdk-sv": "^0.0.12",
		"drizzle-orm": "^0.29.3",
		"formsnap": "^0.4.1",
		"lucia": "^2.7.4",
		"lucide-svelte": "^0.297.0",
		"mode-watcher": "^0.1.2",
		"pg": "^8.11.3",
		"resend": "^2.1.0",
		"tailwind-merge": "^2.1.0",
		"tailwind-variants": "^0.1.18"
	}
}
