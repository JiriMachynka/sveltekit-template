{
	"name": "sveltekit-template",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "pnpm dlx @biomejs/biome lint ./src",
		"lint:apply": "pnpm dlx @biomejs/biome lint --apply ./src",
		"format": "pnpm dlx @biomejs/biome format ./src --write",
		"format:lint": "pnpm dlx @biomejs/biome check ./src",
		"migrate": "drizzle-kit generate:pg --config=drizzle.config.ts",
		"migration:push": "drizzle-kit push:pg --config=drizzle.config.ts",
		"db:studio": "drizzle-kit studio",
		"clean": "node ./scripts/clean.js"
	},
	"devDependencies": {
		"@biomejs/biome": "1.5.3",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/pg": "^8.11.0",
		"autoprefixer": "^10.4.17",
		"drizzle-kit": "^0.20.14",
		"postcss": "^8.4.35",
		"postcss-load-config": "^5.0.3",
		"svelte": "^4.2.10",
		"svelte-check": "^3.6.3",
		"sveltekit-flash-message": "^2.4.1",
		"sveltekit-superforms": "^1.13.4",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.1.0",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-drizzle": "^1.0.1",
		"@lucia-auth/adapter-postgresql": "^3.1.0",
		"bits-ui": "^0.17.0",
		"clsx": "^2.1.0",
		"cmdk-sv": "^0.0.13",
		"drizzle-orm": "^0.29.3",
		"formsnap": "^0.4.3",
		"lucia": "^3.0.1",
		"lucide-svelte": "^0.323.0",
		"mode-watcher": "^0.2.0",
		"oslo": "^1.1.0",
		"pg": "^8.11.3",
		"resend": "^3.1.0",
		"tailwind-merge": "^2.2.1",
		"tailwind-variants": "^0.1.20"
	}
}
